<%= simple_form_for(@company) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :website %>
    <%= f.input :deck %>
    <%= f.input :contact %>
    <%= f.input :stage %>
    <%= f.input :via %>
    <%= f.input :angel_list %>
    <%= f.input :mattermark %>
    <%= f.input :status %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
